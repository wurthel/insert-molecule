— Молекула, в которую производится вставка
molecule = data/proteins/1M0L.pdb

— Результат работы программы
molecule_result = 1M0L_result.pdb

— Вставляемая молекула в Z-матричном представлении
zmolecule = data/zmatrix

— Число первых атомов, вставка которых производится БЕЗ оптимизации
n_without_opt = 0 

— Диапазон атомов, вставка которых производится С оптимизацией
n_with_opt = (1, 21)

— | Путь к скрипту с оптимизацией
opt_path = optimization-script/

— | Скрипт, запускающий оптимизацию
opt_script = sed -i \"s/ASH/ASP/g; s/GLH/GLU/g\" 1M0L.pdb; bash relax.sh

— | PDB файл оптимизируемой молекулы
opt_mol_beh = 1M0L.pdb

— | PDB файл оптимизированной молекулы
opt_mol_aft = 1M0L_0.pdb

— | Файла, куда выводятся требующие оптимизации аминокислоты
opt_resid = N
