— Число первых атомов, вставка которых производится БЕЗ оптимизации
n_without_opt = 0

— Диапазон атомов, вставка которых производится С оптимизацией
n_with_opt = (1, 5)

— | Скрипт, запускающий оптимизацию
opt_script = relax.sh

— | PDB файл оптимизируемой молекулы
