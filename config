— Вставляемая молекула в Z-матричном представлении
zmatrix = 1M0L_zmatrix

— Молекула, в которую производится вставка
molecule = 1M0L.pdb

— Результат работы программы
molecule_result = RESULT.pdb

— Число первых атомов, вставка которых производится БЕЗ оптимизации
n_without_opt = 5

— Диапазон атомов, вставка которых производится С оптимизацией
n_with_opt = (6, 10)

— | PDB файла оптимизируемой молекулы
molecule_opt_beh = M

— | Файла, куда выводятся требующие оптимизации аминокислоты
resid_opt = L

— | PDB файла оптимизированной молекулы
molecule_opt_aft = N